@{
    ViewData["Title"] = "Home Page";
}

<h2 class="text-center">Projects</h2>

<div class="form-group">
    <select class="form-control browser-default custom-select dropdownUsersMenu" id="dropdownUsersMenu">
        <option selected>--</option>
    </select>
</div>

<table id="tableProjects" class="table table-striped table-bordered" style="width:100%">
    <thead>
        <tr>
            <th>
                Id
            </th>
            <th>
                Start Date
            </th>
            <th>
                Time To Start
            </th>
            <th>
                End Date
            </th>
            <th>
                Credits
            </th>
            <th>
                Status
            </th>
        </tr>
    </thead>
    <tbody>        
    </tbody>
</table>

@section Scripts {
    <script src="~/js/views/users/dropdown.js"></script>
    <script src="~/js/views/projects/list.js"></script>
}